#!/usr/bin/env bash
# This script will display the numbers 1-20, 4 will show bad luck from China, 9 will show bad luck from Japan, 17 will show bad luck from Italy

i=0
while [ $i -le 19 ]
do
    echo $i
    case $i in
	3)
	echo 'bad luck from China'
	;;
	8)
	echo 'bad luck from Japan'
	;;
	16)
	echo 'bad luck from Italy'
	;;
    esac
    i$((i + 1))
done
